{{#if isLoggedIn}}
<div class="fondecran" id="contact">
    <h1 id="listeMessage" class="text">Liste des messages</h1>
    <section class="messages">
        {{#each listeContacts}}
        <div class="message-case">
        <p class="text">Nom: {{this.nom}}</p>
        <p class="text">Pr√©nom: {{this.prenom}}</p>
        <p class="text">Mail: {{this.mail}}</p>
        <p class="text">Message: {{this.message}}</p>
        <a class="btn btn-primary" id="button-delete" href="/listeContacts/{{this.id}}">Effacer</a>
        </div>
        {{/each}}
    </section>
</div>
 {{/if}}